{
    "name": "aist/aist-themes",
    "type": "composer-plugin",
    "description": "Zend Framework 2 module that allows developers install &amp; switch between various themes for their projects.",
    "license": "BSD-3-Clause",
    "homepage": "http://projects.mateuszsitek.com/aist-themes",
    "authors": [
        {
            "name": "Mateusz Sitek [MaSi]",
            "email": "webdeveloper@mateuszsitek.com",
            "homepage": "http://mateuszsitek.com",
            "role": "Author, Developer"
        }
    ],
    "keywords": [
        "zf2",
        "theme",
        "template"
    ],
    "require": {
        "php":                          ">=5.3.3",
        "zendframework/zendframework":  ">=2.2.6",
        "composer-plugin-api":          "1.0.0"
        
    },
    "require-dev": {
        "zendframework/zend-developer-tools":   "dev-master",
        "zendframework/zftool":                 "dev-master",
        "zendframework/zenddiagnostics":        ">=1.0.0",
        "phpunit/phpunit":                      ">=4.1.0"
    },
    "autoload": {
        "psr-4": {
            "AistThemes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AistThemesTest\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "class": "AistThemes\\Composer\\ThemeInstallerPlugin"
    }
}
